{
  "name": "adstop",
  "version": "0.0.5",
  "description": "Edit hosts file to beautify web",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "babel-node test/index.js | tspec",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "npm run test -s",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover test/index.js",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jermorin/adstop.git"
  },
  "keywords": [
    "adblock adaway adfuck"
  ],
  "author": "Jérémy Morin <hi@jermor.in>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jermorin/adstop/issues"
  },
  "homepage": "https://github.com/Jermorin/adstop#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "*",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "*",
    "dependency-check": "*",
    "doctoc": "*",
    "eslint": "*",
    "estraverse-fb": "^1.3.1",
    "isparta": "*",
    "rimraf": "*",
    "tap-spec": "*",
    "tape": "*",
    "watch": "*"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "request": "^2.69.0"
  }
}
